            <dns value="localhost"/>
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost/TRealtimeData/"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>

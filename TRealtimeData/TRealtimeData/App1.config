    <add key="CMDbConn" value="Data Source=indialab.tetherfi.com\SQLEXPRESS,14330;Initial Catalog=DOCMT1_TH;User ID=sa;Password=PPP$azhayangadi#2016;"/>
    
    <!-- Log4net Configuration -->
    <add key="Log4NetConfigFile" value="D:\Projects\Tetherfi\Bcms.Realtime\BCMS_RealTime_Dashboard\Log4Net.config"/>

    <!-- Type : CM or SIP -->
    <add key="Type" value="sip"/>

    <!--value= 1 => HA is enabled, value=0=> HA is disabled-->
    <add key="HAEnabled" value="0"/>

    <!--Timewait to read data from CM/SIP in milliseconds-->
